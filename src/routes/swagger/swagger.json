{
	"openapi": "3.0.0",
	"components": {
		"examples": {},
		"headers": {},
		"parameters": {},
		"requestBodies": {},
		"responses": {},
		"schemas": {
			"UserDTO": {
				"properties": {
					"id": {
						"type": "string"
					}
				},
				"required": [
					"id"
				],
				"type": "object",
				"additionalProperties": false
			},
			"IUserSchemaServices": {
				"properties": {
					"id": {
						"type": "string"
					}
				},
				"required": [
					"id"
				],
				"type": "object",
				"additionalProperties": false
			},
			"ICreateUserResponse": {
				"properties": {
					"message": {
						"type": "string",
						"enum": [
							"bot-connectado",
							"Usuario criado",
							"Erro ao criar usuário",
							"Houve um erro ao criar o bot",
							"Erro interno no servidor"
						]
					},
					"user": {
						"$ref": "#/components/schemas/IUserSchemaServices"
					},
					"QRcode": {
						"type": "string"
					},
					"base64": {
						"type": "string"
					},
					"statusBot": {
						"type": "boolean"
					}
				},
				"required": [
					"message"
				],
				"type": "object",
				"additionalProperties": false
			},
			"CreateUserDTO": {
				"properties": {
					"id": {
						"type": "string"
					}
				},
				"required": [
					"id"
				],
				"type": "object",
				"additionalProperties": false
			},
			"IRegenerateQRCodeResponse": {
				"properties": {
					"qr": {},
					"base64": {
						"type": "string"
					},
					"message": {
						"type": "string"
					},
					"error": {
						"type": "string"
					}
				},
				"type": "object",
				"additionalProperties": false
			},
			"IRegenareteQRcodeService": {
				"properties": {
					"id": {
						"type": "string"
					}
				},
				"required": [
					"id"
				],
				"type": "object",
				"additionalProperties": false
			}
		},
		"securitySchemes": {}
	},
	"info": {
		"title": "",
		"contact": {}
	},
	"paths": {
		"/user/get-user": {
			"get": {
				"operationId": "FindUser",
				"responses": {
					"200": {
						"description": "Usuario Encontrado com sucess",
						"content": {
							"application/json": {
								"schema": {
									"allOf": [
										{
											"$ref": "#/components/schemas/UserDTO"
										}
									],
									"nullable": true
								}
							}
						}
					},
					"404": {
						"description": "Usuario nao encontrado"
					}
				},
				"tags": [
					"Users"
				],
				"security": [],
				"parameters": [
					{
						"in": "query",
						"name": "jid",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				]
			}
		},
		"/user/create": {
			"post": {
				"operationId": "CreateUser",
				"responses": {
					"200": {
						"description": "Usuário criado com sucesso",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/ICreateUserResponse"
								}
							}
						}
					},
					"500": {
						"description": "Erro interno do servidor"
					}
				},
				"tags": [
					"Users"
				],
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/CreateUserDTO"
							}
						}
					}
				}
			}
		},
		"/bot/regenerate/qrcode": {
			"post": {
				"operationId": "RegenaretaQRcode",
				"responses": {
					"200": {
						"description": "QR code gerado com sucesso",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/IRegenerateQRCodeResponse"
								}
							}
						}
					},
					"403": {
						"description": "Id necessario"
					},
					"405": {
						"description": "Bot nao encontrado"
					},
					"500": {
						"description": "Houve um erro ao gerar Um novo Qrcode"
					}
				},
				"description": "Gera um novo Qrocde para o bot",
				"tags": [
					"Bot"
				],
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/IRegenareteQRcodeService"
							}
						}
					}
				}
			}
		}
	},
	"servers": [
		{
			"url": "/api"
		}
	]
}